var filter

ovld filter = fn(array -> arr, function -> test) {
    filtered := []

    each (arr, k, v)
        if (test:(v, k)) filtered = append:(filtered, v)

    return filtered
}

ovld filter = fn(hash -> hsh, function -> test) {
    filtered := [::]

    each (hsh, k, v)
        if (test:(v, k)) filtered::(k) = v

    return filtered
}
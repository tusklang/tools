var alloc = fn(int -> length) {

    var ret = []

    cl := clone:(length)

    while (cl > 0) {
        ret = pushback:(ret, 0)
        cl--
    }

    return ret
}

var allocstr = fn(int -> length) {

    var ret = ""

    cl := clone:(length)

    while (cl > 0) {
        ret+=(rune -> 0)
        cl--
    }

    return ret
}


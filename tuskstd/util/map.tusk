var map

ovld map = fn(array -> arr, function -> mapper) {
    mapped := clone:(arr)
    each (arr, k, v) mapped::(k) = mapper:(k, v)
    return mapped
}

ovld map = fn(hash -> hash, function -> mapper) {
    mapped := clone:(hash)
    each (hash, k, v) mapped::(k) = mapper:(k, v)
    return mapped
}
